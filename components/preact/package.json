{
  "name": "preact-particles",
  "version": "2.12.0",
  "description": "Official tsParticles Preact Component - Easily create highly customizable particle, confetti and fireworks animations and use them as animated backgrounds for your website. Ready to use components available also for Web Components, React, Vue.js (2.x and 3.x), Angular, Svelte, jQuery, Riot.js, Solid.js, Inferno.",
  "main": "index.js",
  "unpkg": "umd/particles.js",
  "jsdelivr": "umd/particles.js",
  "types": "index.d.ts",
  "scripts": {
    "prettify:ci:src": "prettier --check ./src/*",
    "prettify:ci:readme": "prettier --check ./README.md",
    "prettify:src": "prettier --write ./src/*",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:ci": "eslint src --ext .js,.jsx,.ts,.tsx",
    "build:debug": "webpack",
    "build": "pnpm run prettify:src && pnpm run lint && cross-env NODE_ENV=production webpack && pnpm run prettify:readme",
    "build:ci": "pnpm run prettify:ci:src && pnpm run lint:ci && cross-env NODE_ENV=production webpack && pnpm run prettify:ci:readme",
    "prepack": "pnpm run build"
  },
  "repository": {
    "url": "https://github.com/tsparticles/preact",
    "directory": "components/preact",
    "type": "git"
  },
